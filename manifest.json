{
  "manifest_version": 3,
  "name": "QuickTask Extension",
  "description": "short tasks manager",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ] 
}